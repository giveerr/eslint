{
  "name": "@giveerr/eslint",
  "version": "2.2.8",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "scripts": {
    "affected": "nx affected --target=build --parallel 10",
    "build": "nx run-many --target=build --parallel 10",
    "clean": "nx run-many --target=clean --parallel 10",
    "fix": "npm-run-all eslint",
    "eslint": "eslint --fix --ext js,jsx,ts,tsx .",
    "prepare": "is-ci || husky install"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "package.json": "npx sort-package-json"
  },
  "devDependencies": {
    "@giveerr/eslint-config": "*",
    "@types/node": "^20.2.5",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "nx": "^16.3.2",
    "rimraf": "^5.0.1",
    "tsup": "^6.5.0",
    "typescript": "^5.1.3"
  }
}
